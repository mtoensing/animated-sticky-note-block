{
  "apiVersion": 2,
  "name": "create-block/simplestickynote",
  "title": "Simple Sticky Note",
  "category": "widgets",
  "icon": "sticky",
  "description": "Leave sticky notes directly on your website using a simple Gutenberg block.",
  "supports": {
    "html": false,
    "align": true,
    "typography": {
      "fontFamily": true,
      "fontSize": true
    },
    "color": {
      "text": true,
      "background": true,
      "gradients": true,
      "link": true
    }
  },
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "p"
    },
    "textAlign": {
      "type": "string",
      "default": "center"
    },
    "align": {
      "type": "string",
      "default": "center"
    }
  },
  "textdomain": "simplestickynote",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}